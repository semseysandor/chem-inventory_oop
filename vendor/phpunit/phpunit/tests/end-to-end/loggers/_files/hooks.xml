         xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/7.1/phpunit.xsd"
         printerClass="PHPUnit\Test\NullPrinter">
    <extensions>
        <extension class="PHPUnit\Test\Extension">
            <arguments>
                <integer>10</integer>
                <string>SOME_STRING</string>
                <double>2.3</double>
            </arguments>
        </extension>
    </extensions>
</phpunit>
